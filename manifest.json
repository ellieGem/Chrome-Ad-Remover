{
    "manifest_version": 2,
    "name": "RemoveAds",
    "description": "Injects js files into webpages, can be used to remove google ads, automatically skip the youtube ads..",
    "version": "1",
    "author": "Allotey",
    "browser_action": {
        "default_title": "Custom js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "jquery.min.js",
                "background.js"
            ]
        }
    ],
    "permissions": ["activeTab"]
}